{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "app",
    "components",
    "constants",
    "hooks",
    "scripts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/__mocks__/*",
    "**/__tests__/*",
    "**/__rsc_tests__/*"
  ]
}
